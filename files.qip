# MiSTer FPGA Files
set_global_assignment -name SYSTEMVERILOG_FILE rtl/Nand2Tetris.v
set_global_assignment -name SDC_FILE Nand2Tetris.sdc
set_global_assignment -name SYSTEMVERILOG_FILE Nand2Tetris.sv

# Elementary Logic Gates
set_global_assignment -name VERILOG_FILE rtl/logic_gates/elementary/AND.v
set_global_assignment -name VERILOG_FILE rtl/logic_gates/elementary/NAND.v
set_global_assignment -name VERILOG_FILE rtl/logic_gates/elementary/NOR.v
set_global_assignment -name VERILOG_FILE rtl/logic_gates/elementary/NOT.v
set_global_assignment -name VERILOG_FILE rtl/logic_gates/elementary/OR.v

# Composite Logic Gates
set_global_assignment -name VERILOG_FILE rtl/logic_gates/composite/XOR.v
